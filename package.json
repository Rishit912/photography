{
  "name": "photography-monorepo",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev": "npm run dev --workspace frontend",
    "build": "npm run build --workspace frontend",
    "preview": "npm run preview --workspace frontend",
    "server": "npm run start --workspace backend",
    "dev:full": "concurrently \"npm run start --workspace backend\" \"npm run dev --workspace frontend\""
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
